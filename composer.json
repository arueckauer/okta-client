{
    "name": "arueckauer/okta-client",
    "type": "library",
    "description": "PHP client for Core Okta API",
    "keywords": [
        "okta",
        "okta-api",
        "okta-client",
        "client"
    ],
    "homepage": "https://github.com/arueckauer/okta-client",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andi RÃ¼ckauer",
            "email": "arueckauer@gmail.com",
            "homepage": "https://arueckauer.github.io/",
            "role": "Project Lead"
        }
    ],
    "require": {
        "php": "^7.4 || ~8.0.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.5.5",
        "laminas/laminas-config-aggregator": "^1.7",
        "laminas/laminas-servicemanager": "^3.10",
        "symfony/console": "^5.3"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.3",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.11"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "laminas": {
            "config-provider": [
                "OktaClient\\ConfigProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "OktaClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OktaClientTest\\": "test"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "bin/okta-client"
    ]
}
